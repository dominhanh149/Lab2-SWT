<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>jspfilter</filter-name>
        <filter-class>filter.jspfilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>jspfilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.auth.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.auth.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <servlet-class>controller.manage.ProductionPlanCreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanListController</servlet-name>
        <servlet-class>controller.manage.ProductionPlanListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanDetailController</servlet-name>
        <servlet-class>controller.manage.ProductionPlanDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanDeleteController</servlet-name>
        <servlet-class>controller.manage.ProductionPlanDeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Assignment</servlet-name>
        <servlet-class>controller.manage.ProductionPlanAssignmentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanUpdateController</servlet-name>
        <servlet-class>controller.manage.ProductionPlanUpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleCreate</servlet-name>
        <servlet-class>controller.manage.ScheduleCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Attendent</servlet-name>
        <servlet-class>controller.manage.AttendentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feature</servlet-name>
        <servlet-class>controller.manage.FeaturePlan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorkInfor</servlet-name>
        <servlet-class>controller.manage.WorkInfor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSalary</servlet-name>
        <servlet-class>controller.manage.ViewSalary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>controller.auth.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeView</servlet-name>
        <servlet-class>controller.manage.ViewEmployee</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <url-pattern>/productionPlan/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanListController</servlet-name>
        <url-pattern>/productionPlan/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanDetailController</servlet-name>
        <url-pattern>/productionPlan/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanDeleteController</servlet-name>
        <url-pattern>/productionPlan/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Assignment</servlet-name>
        <url-pattern>/productionPlan/assignment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductionPlanUpdateController</servlet-name>
        <url-pattern>/productionPlan/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleCreate</servlet-name>
        <url-pattern>/productionPlan/assignment/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Attendent</servlet-name>
        <url-pattern>/productionPlan/attendent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feature</servlet-name>
        <url-pattern>/productionPlan/feature</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorkInfor</servlet-name>
        <url-pattern>/productionPlan/workinfor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewSalary</servlet-name>
        <url-pattern>/productionPlan/viewsalary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeView</servlet-name>
        <url-pattern>/viewemp</url-pattern>
    </servlet-mapping>
</web-app>
